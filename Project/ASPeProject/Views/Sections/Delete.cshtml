@model SurveyProject.tblSection

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()



    <div class="card">
        <div class="header">
            <h2>Delete</h2>
            <small>Are you sure you want to delete this?</small>
        </div>
        <div class="body clearfix">
            <div class="col-lg-12">
                
                


                    <dl class="dl-horizontal">
                        <dt>
                            Section Name
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.SectionName)
                        </dd>

                        <dt>
                            Class Name
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.tblClass.ClassName)
                        </dd>

                    </dl>
                


                    <div class="form-group">
                        <div class="col-lg-2 col-md-4 col-sm-6 col-xs-12">
                            <input type="submit" value="Delete" class="btn btn-block bg-red waves-effect" />
                        </div>
                        <div class="col-lg-2 col-md-4 col-sm-6 col-xs-12">
                            @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-block bg-blue waves-effect" })
                        </div>
                    </div>

                

            </div>
        </div>
    </div>
}


