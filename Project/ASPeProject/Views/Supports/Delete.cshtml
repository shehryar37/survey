@model SurveyProject.tblSupport

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()



    <div class="card">
        <div class="header">
            <h2>Delete</h2>
            <small>Are you sure you want to delete this?</small>
        </div>
        <div class="body clearfix">
            <div class="col-lg-12">



                <dl class="dl-horizontal">
                    <dt>
                        Support Number
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.SupportNumber)
                    </dd>

                    <dt>
                        Support Email
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.SupportEmail)
                    </dd>

                    <dt>
                        Support Address
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.SupportAddress)
                    </dd>

                    <dt>
                        Support Person
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.SupportPerson)
                    </dd>

                    <dt>
                        Reporting DateTime
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.ReportingDateTime)
                    </dd>

                    <dt>
                        Support Active
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.SupportActive)
                    </dd>

                </dl>


                <div class="form-group">
                    <div class="col-lg-2 col-md-4 col-sm-6 col-xs-12">
                        <input type="submit" value="Delete" class="btn btn-block bg-red waves-effect" />
                    </div>
                    <div class="col-lg-2 col-md-4 col-sm-6 col-xs-12">
                        @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-block bg-blue waves-effect" })
                    </div>
                </div>




            </div>
        </div>
    </div>
}



